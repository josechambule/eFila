	<variable name="totalPills" class="java.lang.Long">
		<variableExpression><![CDATA[new Long(
$F{openingpills}.longValue()
+ $F{returned}.longValue()*$P{packsize}.intValue()
+ $F{returnedpills}.longValue()
- $F{destroyed}.longValue()*$P{packsize}.intValue()
- $F{destroyedpills}.longValue()
)
+ $V{somaUnidadesRecebidas}*$P{packsize}.intValue() + $V{UnidadesRecebidas}*$P{packsize}.intValue()
- $V{somaUnidadesDispensadas}*$P{packsize}.intValue() - $V{UnidadesDispensadas}*$P{packsize}.intValue()
- $V{somaUnidadesAjustadas} - $V{UnidadesAjustadas}]]></variableExpression>
		<initialValueExpression><![CDATA[new Long(0)]]></initialValueExpression>
	</variable>
	<variable name="totalString" class="java.lang.Long">
		<variableExpression><![CDATA[$V{totalPills}.longValue()%$P{packsize}.intValue() == 0 ?
$V{totalPills}.longValue()/$P{packsize}.intValue()
:$V{totalPills}.longValue()/$P{packsize}.intValue() +1]]></variableExpression>
	</variable>
	<variable name="somaUnidadesRecebidas" class="java.lang.Long">
		<variableExpression><![CDATA[$V{UnidadesRecebidas} + $V{somaUnidadesRecebidas}]]></variableExpression>
